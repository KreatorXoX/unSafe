<% layout('layouts/boilerplate') %>
<div class="row">
  <div class="col-8 offset-2">
    <div class="alert alert-danger" role="alert">
      <h5 class="alert-heading"><%= err.message %></h5>
      <% if (process.env.NODE_ENV !== 'production') { %>
      <span><%= err.stack %></span>
      <% } %>
    </div>
  </div>
</div>
